{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "drip-form-validator": "^0.0.6",
    "gatsby": "^1.6.5",
    "gatsby-link": "^1.6.5",
    "gatsby-plugin-react-helmet": "^1.0.1",
    "normalize.css": "^7.0.0",
    "polished": "^1.3.0",
    "ramda": "^0.24.1",
    "rebass": "^1.0.3",
    "recompose": "^0.24.0",
    "styled-system": "^1.0.0-12"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "test": "ava",
    "tdd": "ava --watch"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-preset-env": "^1.6.0",
    "bluebird": "^3.5.0",
    "gatsby-plugin-catch-links": "^1.0.2",
    "gatsby-plugin-sharp": "^1.6.0",
    "gatsby-plugin-styled-components": "^1.0.1",
    "gatsby-remark-copy-linked-files": "^1.5.0",
    "gatsby-source-filesystem": "^1.4.1",
    "gatsby-transformer-json": "^1.0.1",
    "gatsby-transformer-remark": "^1.6.3",
    "prettier": "^1.5.2",
    "slash": "^1.0.0"
  },
  "ava": {
    "files": [
      "src/**/*.tests.js"
    ],
    "source": [
      "src/**/*.{js,jsx}"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "browserslist": [
    "IE >= 11",
    "last 2 versions"
  ]
}
